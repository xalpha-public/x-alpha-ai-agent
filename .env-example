# Flask Server Port (optional, defaults to 8080 in demo_app.py)
FLASK_PORT="8080"

# Coinbase CDP API Key ID and Secret
CDP_API_KEY_ID=
CDP_API_KEY_SECRET=

# Ethereum RPC Provider URL (e.g., from Infura, Alchemy, or your own node)
PROVIDER_URL="YOUR_ETHEREUM_RPC_URL_HERE"
CHAIN_ID="8453"
# Private key for the wallet
PRIVATE_KEY=""
